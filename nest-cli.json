{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/crop-watch-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/crop-watch-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/crop-watch-api",
  "projects": {
    "crop-watch-api": {
      "type": "application",
      "root": "apps/crop-watch-api",
      "entryFile": "main",
      "sourceRoot": "apps/crop-watch-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crop-watch-api/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "soil_sensor_service": {
      "type": "application",
      "root": "apps/soil_sensor_service",
      "entryFile": "main",
      "sourceRoot": "apps/soil_sensor_service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/soil_sensor_service/tsconfig.app.json"
      }
    },
    "dtos": {
      "type": "library",
      "root": "libs/dtos",
      "entryFile": "index",
      "sourceRoot": "libs/dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dtos/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    }
  }
}